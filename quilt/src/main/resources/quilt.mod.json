{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "rideableravagers",
    "version": "${version}",
    "metadata": {
      "name": "${mod_name}",
      "description": "${mod_description}",
      "contributors": {
        "${mod_author}": "Owner"
      },
      "contact": {
        "homepage": "",
        "issues": "",
        "source": ""
      },
      "license": "AGPL-3.0",
      "icon": "assets/rideableravagers/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "com.github.salandora.rideableravagers.RideableRavagersMod"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=${quilt_loader_version}"
      },
      {
        "id": "minecraft",
        "versions": ">=${minecraft_version}"
      },
      {
        "id": "java",
        "versions": ">=17"
      }
    ]
  },
  "mixin": [
    "rideableravagers.common.mixins.json"
  ]
}
